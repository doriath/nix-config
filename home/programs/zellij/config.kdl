default_shell "nu"
theme "gruvbox"
copy_command "xclip -selection clipboard"
keybinds {
    unbind "Ctrl b"
    shared_except "session" "locked" {
        unbind "Ctrl o"
        bind "Ctrl a" { SwitchToMode "Session"; }
    }
}
